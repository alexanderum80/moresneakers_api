{
  "name": "node-ddd-boilerplate",
  "version": "1.0.0",
  "description": "RESTful api Domain Driven Design",
  "main": "index.js",
  "engines": {
    "node": ">=9",
    "npm": ">=5"
  },
  "scripts": {
    "start-dev": "cross-env NODE_OPTIONS='--inspect' NODE_PATH=. NODE_ENV=development node --max_old_space_size=4096 index.js",
    "start-staging": "cross-env NODE_PATH=. NODE_ENV=staging node --max_old_space_size=8192 index.js",
    "start-prod": "NODE_PATH=. NODE_ENV=production node --max_old_space_size=8192 index.js",
    "start-cluster": "node cluster.js",
    "debug": "cross-env NODE_PATH=. NODE_ENV=development nodemon --inspect",
    "gc": "git-cz",
    "test": "standard --verbose | snazzy && npm run test:cov",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test:cov": "cross-env NODE_PATH=. NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha -- --exit",
    "test:specs": "cross-env NODE_PATH=. NODE_ENV=test mocha --exit",
    "db:refresh": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:refresh-test": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "migrate": "sequelize db:migrate",
    "migrate:test": "sequelize db:migrate --env test",
    "migrate:prod": "sequelize db:migrate --env production",
    "seed": "cross-env NODE_ENV=development sequelize db:seed:all --seeders-path=./src/infra/sequelize/seeders/development",
    "seed:prod": "cross-env NODE_ENV=production sequelize db:seed:all --seeders-path=./src/infra/sequelize/seeders/production",
    "seed:test": "cross-env NODE_ENV=test sequelize db:seed:all --env test --seeders-path=./src/infra/sequelize/seeders/test",
    "lint": "standard --verbose | snazzy",
    "lint:staged": "lint-staged",
    "fresh-install": "rm -rf node_modules && npm cache clean --force && npm install",
    "semantic-release": "semantic-release"
  },
  "lint-staged": "nmp run lint --",
  "pre-commit": "lint:staged",
  "author": "soldev",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.27.2",
    "@sentry/tracing": "^5.27.2",
    "awilix": "3.0.7",
    "awilix-express": "1.0.0",
    "aws-sdk": "^2.422.0",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.1",
    "bitly": "^6.0.8",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "cross-env": "5.1.4",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "express-status-monitor": "1.0.1",
    "faker": "4.1.0",
    "googleapis": "^39.2.0",
    "http-status": "1.0.1",
    "ioredis": "^5.0.4",
    "jimp": "^0.16.1",
    "jsonwebtoken": "8.2.1",
    "moment": "2.22.1",
    "moment-timezone": "0.5.16",
    "morgan": "1.9.0",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "mysql2": "^1.6.4",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.6",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "path": "^0.12.7",
    "pg": "7.4.1",
    "ramda": "0.25.0",
    "sequelize": "4.37.6",
    "sequelize-cli": "^5.4.0",
    "slugify": "^1.6.0",
    "swagger-jsdoc": "1.9.7",
    "tcomb": "3.2.25",
    "uniqid": "^5.0.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "axios": "^0.20.0",
    "chai": "4.1.2",
    "commitizen": "2.9.6",
    "coveralls": "3.0.0",
    "cz-conventional-changelog": "2.1.0",
    "istanbul": "1.0.0-alpha.2",
    "lint-staged": "7.0.5",
    "mocha": "5.1.1",
    "mocha-lcov-reporter": "1.3.0",
    "node-fetch": "^2.6.1",
    "npm-check-updates": "2.14.2",
    "npx": "10.2.0",
    "pre-commit": "1.2.2",
    "semantic-release": "15.4.1",
    "supertest": "3.0.0",
    "snazzy": "^9.0.0",
    "standard": "^17.0.0"
  },
  "standard": {
    "ignore": [
      "/public/docs"
    ],
    "globals": [
      "request",
      "app",
      "expect",
      "describe",
      "it",
      "config",
      "beforeEach"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/solutiondeveloppeur/moresneakers.git"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "release": {
    "publish": [
      "@semantic-release/github"
    ]
  }
}
